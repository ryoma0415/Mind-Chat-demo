    "postprocess": {
      "normalize_spaces": true,
      "append_punctuation": true,
      "use_timing": true,
      "sentence_gap_sec": 0.4
    }
  },
  "embedding": {
    "model_path": null
  },
  "voicevox": {
    "base_url": "http://127.0.0.1:50021"
  }
