{
  "app": {
    "default_mode_key": "plain_chat"
  },
  "llm": {
    "model_path": null,
    "max_context_tokens": 4096,
    "max_response_tokens": 512,
    "temperature": 0.7,
    "top_p": 0.9,
    "gpu_layers": 0,
    "threads": null
  },
  "history": {
    "max_conversations": 60,
    "max_favorites": 50
  },
